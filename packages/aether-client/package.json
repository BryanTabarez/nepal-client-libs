    "lint": "tslint -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "interactive": "npm run build && node -i -e \"const AetherClient = require('./dist/commonjs/index.js').AetherClient;\""
