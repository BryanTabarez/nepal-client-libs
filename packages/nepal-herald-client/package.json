{
  "name": "@al/herald",
  "version": "1.0.20",
  "license": "MIT",
  "description": "A lightweight client library for interacting with the Herald API",
  "author": {
    "name": "Gisler Garces",
    "email": "ggarces@alertlogic.com"
  },
  "keywords": [
    "Alert Logic",
    "Client",
    "Service"
  ],
  "maintainers": [
    {
      "name": "Gisler Garces",
      "email": "ggarces@alertlogic.com"
    },
    {
      "name": "Alert Logic NPM Team",
      "email": "npm@alertlogic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alertlogic/nepal-herald-client"
  },
  "main": "./dist/umd/index.js",
  "module": "./dist/commonjs/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "build":"rollup -c ../../rollup.config.ts",
    "prebuild": "npm run clean && npm run lint",
    "lint": "tslint -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "interactive": "npm run build && node -i -e \"const HeraldClient = require('./dist/commonjs/index.js').AlHeraldClient;\"",
    "interactive-dev": "npm run build-dev && node -i -e \"const HeraldClient = require('./dist/commonjs/index.js').AlHeraldClient;\""
  },
  "dependencies": {
    "@al/core": "*"
  },
  "peerDependencies": {
    "@al/core": "*"
  },
  "files": [
    "dist"
  ]
}
